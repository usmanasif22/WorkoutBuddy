<div class="workouts">
<% unless @workouts.blank? %>
<% @workouts.each do |workout| %>
  <div class="card " style="text-decoration: none "><%= link_to workout.title, workout_path(workout) %>
    <div class="date"><%= time_ago_in_words(workout.created_at)+" ago"  %></div>
  </div>
  <% end %>
<% else %>
 <div class="details " >
  <%= @message  %>
</div>
<% end %>
</div>
